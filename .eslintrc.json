{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "require-jsdoc": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "indent": 0
  }
}
